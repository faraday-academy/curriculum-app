{
  "name": "curriculum-back",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "app.js",
  "exports": {
    ".": "./app.js",
    "./db": "./db/index.js",
    "./server": "./server/index.js",
    "./utils": "./utils/index.js"
  },
  "engines": {
    "node": ">=20.15.0",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest --coverage --passWithNoTests",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "setup": "concurrently \"npm i\" \"cp sample-env .env\"",
    "start": "cross-env NODE_ENV=local npx nodemon --exec babel-node app.js",
    "debug": "npx nodemon --exec babel-node --inspect app.js",
    "preprod": "npm i --production",
    "prod": "cross-env NODE_ENV=production node app.js",
    "db:seed": "node db/seeder/seed.js",
    "db:clean": "node db/seeder/clean.js",
    "prepare": "cd .. && husky curriculum-back/.husky"
  },
  "keywords": [],
  "author": "Gwendolyn Faraday <gwenfaraday@pm.me> (https://gwenfaraday.com/)",
  "license": "MIT",
  "dependencies": {
    "@babel/eslint-parser": "^7.24.7",
    "@treblle/express": "^0.0.8",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mailgun-js": "^0.22.0",
    "module-alias": "^2.2.3",
    "mongoose": "^8.4.3",
    "mongoose-paginate-v2": "^1.8.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/node": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@faker-js/faker": "^8.4.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.5.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0"
  },
  "_moduleAliases": {
    "@db": "db"
  }
}