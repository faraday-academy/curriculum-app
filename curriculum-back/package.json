{
  "name": "curriculum-back",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "app.js",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore .",
    "test": "jest --coverage",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "setup": "concurrently \"npm i\" \"cp sample-env .env\"",
    "start": "cross-env NODE_ENV=local npx nodemon -r esm app.js",
    "debug": "npx nodemon --inspect app.js",
    "preprod": "npm i --production",
    "prod": "cross-env NODE_ENV=production node app.js",
    "db:seed": "node db/seeder/seed.js",
    "db:clean": "node db/seeder/clean.js",
    "prepare": "cd .. && husky curriculum-back/.husky"
  },
  "keywords": [],
  "author": "Gwendolyn Faraday <gwenfaraday@pm.me> (https://gwenfaraday.com/)",
  "license": "MIT",
  "dependencies": {
    "@treblle/express": "^0.0.7",
    "bcrypt": "5.0.0",
    "concurrently": "^7.0.0",
    "cors": "2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^16.4.5",
    "esm": "^3.2.25",
    "express": "4.17.1",
    "helmet": "3.21.2",
    "jsonwebtoken": "8.5.1",
    "mailgun-js": "0.22.0",
    "module-alias": "2.2.2",
    "mongoose": "5.8.3",
    "mongoose-paginate-v2": "1.3.9",
    "morgan": "1.10.0",
    "nodemon": "^3.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^9.5.0",
    "husky": "^9.0.11",
    "jest": "25.1.0"
  },
  "_moduleAliases": {
    "@db": "db"
  }
}